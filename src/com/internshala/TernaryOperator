package com.internshala;

public class TernaryOperator {

	public static void main(String[] args) {
		int num1 = 50;
		int num2 = 60;
		int result;
		int result1;
		//print larger value
		result = num1>num2?num1:num2;
		System.out.println(result);
		result1 = num1<num2?num1:num2;
		System.out.println(result1);

	}

}
